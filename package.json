{
  "name": "stableflow-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "postbuild": "cp src/common/utils/alchemy-wrapper.mjs dist/common/utils/alchemy-wrapper.mjs",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:staging": "cross-env NODE_ENV=staging nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --config jest.config.js --watch",
    "test:cov": "cross-env NODE_ENV=test jest --config jest.config.js --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:e2e": "cross-env NODE_ENV=test jest --config jest.config.js --testRegex \".e2e-spec.ts$\"",
    "test:all": "cross-env NODE_ENV=test jest --config jest.config.js --testRegex \".(e2e-spec|spec).ts$\"",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate -d src/config/db.config.ts src/migrations/AddMigration",
    "migration:run": "yarn typeorm migration:run -d src/config/db.config.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/config/db.config.ts"
  },
  "dependencies": {
    "@aa-sdk/core": "^4.13.0",
    "@account-kit/infra": "^4.13.0",
    "@account-kit/smart-contracts": "^4.13.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bull": "^4.10.4",
    "@types/multer": "^1.4.12",
    "axios": "^1.8.3",
    "bcryptjs": "^3.0.2",
    "bip39": "^3.1.0",
    "bull": "^4.16.5",
    "bullmq": "^5.40.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.5.1",
    "dotenv": "^16.4.7",
    "esm": "^3.2.25",
    "ioredis": "^5.5.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "viem": "^2.23.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@upstash/redis": "^1.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
